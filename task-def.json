{
    "family": "local-runner-fargate",
    "networkMode": "awsvpc",
    "executionRoleArn": "arn:aws:iam::441944353256:role/ecsTaskExecutionRole",
    "containerDefinitions": [{
        "name": "local-runner",
        "image": "441944353256.dkr.ecr.us-west-2.amazonaws.com/v2.0.2-local-runner:latest",
        "portMappings": [{
            "containerPort": 80,
            "hostPort": 80,
            "protocol": "tcp"
        }],
        "essential": true
    }], 
    "requiresCompatibilities": [
        "FARGATE"
    ],  
    "cpu": "256",
    "memory": "512"
}   
